%h1 Listing reports
.CSSTableGenerator
  %table 
 
    %tr
      %td Date
      %td Patientid
      %td Reporter
      %td NextDate
      %td
      %td
      %td
      
 
    - @reports.each do |report|
      %tr
        %td= report.date
        %td= report.patientId
        %td= report.reporter
        %td= report.nextDate
        %td= link_to 'Show', report
        %td= link_to 'Edit', edit_report_path(report)
        %td= link_to 'Destroy', report, :method => :delete, :data => { :confirm => 'Are you sure?' }

%br

= link_to 'New Report', new_report_path
